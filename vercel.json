{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["config/**/*", "routes/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api-docs(.*)",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  